    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch frontend dev",
            "type": "node-terminal",
            "request": "launch",
            "command": "pnpm run dev",
            "cwd": "${workspaceRoot}/frontend"
        },
        {
            "name": "Launch API",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceRoot}",
            "env": { "VIKUNJA_SERVICE_INTERFACE": "127.0.0.1:3456" },
            "args": []
        },
        {
            "name": "Debug Bucket_ReadAll Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/pkg/models",
            "args": [
                "-test.v",
                "-test.run",
                "^TestBucket_ReadAll/normal$"
            ]
        },
        {
            "name": "Debug TestProjectV2TasksGetAll Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/pkg/webtests",
            "args": [
                "-test.v",
                "-test.run",
                "^TestProjectV2TasksGetAll/Forbidden$"
            ]
        }
    ]
}
